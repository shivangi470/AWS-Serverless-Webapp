// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
class node{
public:
int value;
node *next;
node(int num)
{
    value=num;
    next=NULL;
}
};
int main() {
    // Write C++ code here
     int arr[]={1,2,3,4,5};
     node *head=NULL;
    int n= sizeof(arr)/sizeof(arr[0]);
    for(int i=0;i<n;i++)
     {
          if(head==NULL)
          {
              head=new node(arr[i]);
          }
         else{
             node *tail=head;
             while(tail->next!=NULL)
                 {
                     tail=tail->next;
                 }
             tail->next=new node(arr[i]);
             
             }

     }


     node *temp=head;

    while(temp != NULL)
        {
            cout<<temp->value;
            temp=temp->next;
        }
     

    return 0;
}
